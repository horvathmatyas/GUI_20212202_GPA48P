<Window x:Class="SpaceBaloons.Loads"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpaceBaloons"
        mc:Ignorable="d"
        Title="Loads" Height="259" Width="355">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="185*"/>
            <RowDefinition Height="58*"/>
        </Grid.RowDefinitions>
        <ListBox Grid.Row="0" Name="lb_saves" SelectionChanged="lb_saves_SelectionChanged_1">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <Label Content="{Binding}"/>
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>
        <Button Grid.Row="1" Content="Load" Click="Button_Click"/>
    </Grid>
</Window>
